<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>coronatickets.uy</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>controladores.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controladores.Login</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>Espectaculos</servlet-name>
    	<servlet-class>controladores.Espectaculos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>controladores.Logout</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>Altaespectaculo</servlet-name>
    	<servlet-class>controladores.Altaespectaculo</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>AgregarEspectaculoaPaquete</servlet-name>
    	<servlet-class>controladores.AgregarEspectaculoaPaquete</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>DetallesEspectaculo</servlet-name>
    	<servlet-class>controladores.DetallesEspectaculo</servlet-class>
    </servlet>
     <servlet>
    	<servlet-name>DetallesArtista</servlet-name>
    	<servlet-class>controladores.DetallesArtista</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>Espectaculos</servlet-name>
    	<url-pattern>/espectaculos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>Altaespectaculo</servlet-name>
        <url-pattern>/altaespectaculo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarEspectaculoaPaquete</servlet-name>
        <url-pattern>/agregarEspectaculoaPaquete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetallesEspectaculo</servlet-name>
        <url-pattern>/detallesEspectaculo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetallesArtista</servlet-name>
        <url-pattern>/detallesArtista</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>